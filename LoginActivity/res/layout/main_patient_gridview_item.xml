<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/qianlanse" >

    <FrameLayout
        android:id="@+id/frameLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <ImageView
            android:id="@+id/huanzhe_touxiang"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/ipad_female_head" />

        <ImageView
            android:id="@+id/huanzhe_hulijibie"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="40dp"
            android:src="@drawable/tejihuli"
            android:contentDescription="@string/app_name"
            android:visibility="visible" />
    </FrameLayout>

    <TextView
        android:id="@+id/huanzhe_nianling"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/huanzhe_xingming"
        android:layout_below="@+id/huanzhe_xingming"
        android:gravity="center"
        />

    <TextView
        android:id="@+id/huanzhe_xingming"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/frameLayout1"
        android:layout_marginLeft="5dp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/huanzhe_chuanghao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="85dp"
        android:layout_marginTop="15dp"
         />

    <TextView
        android:id="@+id/huanzhe_zhuyuan_tianshu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/huanzhe_chuanghao"
        android:layout_marginLeft="85dp"
        android:layout_marginTop="10dp"
         />

    <TextView
        android:id="@+id/huanzhe_zhuyuan_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/huanzhe_zhuyuan_tianshu"
        android:layout_marginLeft="85dp"
        android:layout_marginTop="10dp"
        />

</RelativeLayout>