<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/scroll_tab_column_width"
    android:layout_height="50dp"
    android:orientation="horizontal" 
    
   >


    <LinearLayout
        android:id="@+id/tizheng_value_common"
        android:layout_width="@dimen/scroll_tab_column_width"
        android:layout_height="50dp"
        
         android:background="@drawable/bg_edittext"
        android:orientation="horizontal" >

       <Spinner
        android:id="@+id/tizheng_fangshi"
        android:layout_width="0.0dp"
        android:layout_height="fill_parent"
        
        android:layout_weight="1.5"
      android:background="@drawable/bg_edittext"
        android:spinnerMode="dialog"
        android:textSize="13sp" />
		<Spinner
            android:id="@+id/tizheng_value_spinner"
            android:layout_width="0.0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:background="#FFFFFF"
           
            android:spinnerMode="dialog"
            android:textSize="14sp"
            android:visibility="gone" />
        <TextView
            android:id="@+id/tizheng_value"
            android:layout_width="0.0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:editable="true"
            android:inputType="text"
            android:padding="0dp"
           
            android:textSize="13sp"
            android:gravity="center" />

        <TextView
            android:id="@+id/tizheng_xiexian"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:gravity="center_vertical|right"
            android:text="@string/tizheng_text_xiegang"
            android:textSize="13sp"
                      />

        <TextView
            android:id="@+id/tizheng_value_2"
            android:layout_width="0.0dp"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:hint=""
            android:padding="0dp"
            android:textSize="13sp"
           
           />

        <TextView
            android:id="@+id/tizheng_jianchazhi_danwei"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:gravity="center_vertical|right"
            android:text="@string/tizheng_text_jianchazhi"
            android:textSize="16sp" 
            android:paddingRight="10dp"/>
    </LinearLayout>

  
</LinearLayout>