html,body,div,h1, h2, h3, h4, h5, h6, p, ul, li, dl, dt, dd, form, input, button ,a {
	margin:0;
	padding:0;
	list-style:none;
	text-decoration:none;
}
body {
	margin:10px;
	float:left;
	padding:0px;
	width:98%;
	min-width:720px;
	background-color:#FFF;
}

.clear {
	clear:both;
	height:1px;
}

.login_infor {
	margin:0 auto;
	margin-top:25px;
	width:100%;
	height:50px;
	line-height:40px;
	font-size:16px;
	font-weight:normal;
	color:#485100;
	padding:0;
	overflow:hidden;
	background:url(./images/login_info_title.png) no-repeat;
}
.login_infor span {
	color:#FFF;
	float:left;
	text-align:center;
	font-size:14px;
	font-weight:600;
	width:110px;
	height:35px;
	line-height:35px;
	letter-spacing:3px;
}
.login_infor p {
	float:left;
	margin-left:20px;
	font-size:20px;
	letter-spacing:2px;
}

.content_left {
	float:left;
	margin-top:10px;
	background:url(./images/content_title.png) no-repeat;
	border:1px solid #50B4E5;
	width:49%;
	display:inline;
}
.user_infor {
	margin:15px 10px 10px 20px;
}
.user_infor p{
	margin-top:-10px;
	margin-left:30px;
	color:#FFF;
	font-size:16px;
	font-weight:600;
	letter-spacing:2px;
}
.user_infor img{
	float:left;
	margin-top:30px;
}
.user_infor ul{
	float:left;
	margin-top:20px;
	margin-left:30px;
}
.user_infor li {
	height:30px;
	line-height:30px;
	font-size:14px;
	color:#819001;
	border-bottom:dotted 1px #666;
}
.user_infor li span{
	color:black;
}
.user_infor li label, .usermanager_infor li label {
	color:#485100;
}

.content_right {
	width:49%;
	float:right;
	margin-top:10px;
	background:url(./images/content_title.png) no-repeat;
	border:1px solid #50B4E5;
}

.system_notice {
	padding-bottom:10px;
}
.system_notice dt {
	height:24px;
	line-height:28px;
	color:#000;
	font-size:14px;
	font-weight:600;
	letter-spacing:2px;
	padding-left:50px;
	margin-bottom:10px;
	font-weight:bold;
}
.system_notice dd {
	line-height:20px;
	padding-left:15px;
	margin:2px;
	border-bottom:dotted 1px #666;
}
.system_notice dd a {
	font-size:14px;
	color:#000;
	cursor:pointer;
}
.system_notice dd a:hover {
	color:#6d7b00;
}

.content_bottom{
	float:left;
	margin-top:10px;
	background:url(./images/content_title.png) no-repeat;
	border:1px solid #50B4E5;
	width:100%;
	display:inline;
	padding-bottom:10px;
}
.content_bottom p{
	margin-top:3px;
	margin-left:45px;
	color:#FFF;
	font-size:14px;
	font-weight:600;
	letter-spacing:2px;
}
.content_bottom img{
	float:left;
	margin-left:20px;
	margin-top:30px;
}
.content_bottom .bottom_info{
	float:left;
	margin-left:20px;
	margin-top:30px;
}
.content_bottom dt{
	line-height:20px;
	margin:2px;
	border-bottom:dotted 1px #666;
}
.content_bottom dt a{
	color:red;
	font-size:14px;
	line-height:25px;
	cursor:pointer;
}
.content_bottom dt a:hover{
	color:#6d7b00;
}

.content_div {
	margin:5px;
	padding:0px;
	width:99%;
	height:auto;
	overflow:hidden;
	float:left;
	background:#fbfdee;
	border:1px solid #e3ed93;
}
.left_menu {
	margin:0px;
	width:202px;
	height:300px;
	float:left;
	border-right:1px solid #e3ed93;
	background:#f5f9d7;
	padding:10px 0;
}
.left_menu li {
	padding-left:5px;
}
.left_menu li a {
	display:block;
	height:36px;
	line-height:38px;
	font-size:18px;
	color:#333;
	padding-left:20px;
	background:url(./images/user_05.jpg) no-repeat 8px center;
}
.left_menu li a:hover, #current_hover {
	font-weight:bold;
	color:#485100;
	border-left:1px solid #e8f0a5;
	border-top:1px solid #e8f0a5;
	border-bottom:1px solid #e8f0a5;
	position:relative;
	left:1px;
	background:#fbfdee url(./images/user_06.jpg) no-repeat 180px center;
}
.right_content {
	margin:10px;
	padding:0px;
	float:left;
	overflow:hidden;
	width:485px;
}