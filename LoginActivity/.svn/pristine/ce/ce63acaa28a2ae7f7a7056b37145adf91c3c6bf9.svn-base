	float:left;
	cursor:pointer;
}
.jian_btn {
	margin-top:10px;
	width:40px;
	height:40px;
	background:url(./images/jian_jilu.png) no-repeat left center;
	border:0px;
	float:left;
	cursor:pointer;
}
.edit_textarea_title {
	color:#1987D4;
}	

.bottom_quick_link_menu {
	position:fixed;
	bottom:0px;
	z-index:100;
	font-size:16px;
	font-weight:bold;
	padding:5px;
	margin:0px;
	background-color:#27A3F9;
	border:2px dashed #00C;
}
*+html .bottom_quick_link_menu {
	position:fixed;
	bottom:0px;
	z-index:100;
	font-size:16px;
	font-weight:bold;
	padding:15px;
	margin:0px;
	background-color:#2BA5FA;
	border:2px dashed #00C;
}
.bottom_quick_link_menu ul{
	list-style:none;
	padding:0px;
	margin:0px;
	}
.bottom_quick_link_menu ul li {
	list-style:none;
	float:left;
	width:84px;
	border-right:2px dashed #666;
	text-align:center;
}
.bottom_quick_link_menu ul li a {
	color:#00008B;
	display:block;
	text-decoration:none;
}
.bottom_quick_link_menu ul li a:hover {
	color:#FFF;
}


/*注册页面*/
.reg_header { margin:0 auto; height:auto; overflow:hidden; width:100%; border-bottom:1px solid #CCC;}
.reg_header_conteng {margin:0 auto; margin-top:10px; margin-bottom:10px; width:900px;}
.reg_buzhou {margin:0 auto; margin-top:30px; height:37px; width:790px;background:url(./images/regist-process.png) no-repeat center 0px; }
