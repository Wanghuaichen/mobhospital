body {
	font-family:"宋体"/*, Arial, Helvetica, sans-serif*/;
	font-size:20px;
	line-height:2;
	margin:0px;
	padding:0px;
	background-color:white;
}

p{
	padding:0px;
	margin:0px;
}
ol,ul{
	margin:0;
}
li{
	*text-indent:72px;
}

table{
	table-layout:fixed;
	*border-collapse:collapse;
	empty-cells:show;
	word-wrap:break-word;
	overflow:hidden;
}

.page {
	z-index:100;
	background:rgba(255,255,255,0.8);
	margin:0px auto 0 auto;
	padding-top:0px;
	padding-bottom:45px;
	width:720px;
	position:relative;
}

.page_break {
	page-break-before:always;
}

