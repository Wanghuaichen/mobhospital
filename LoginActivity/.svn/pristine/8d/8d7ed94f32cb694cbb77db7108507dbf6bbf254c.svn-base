@charset "utf-8";
html, body {
	font-size:12px;
	font-family:Arial, Helvetica, sans-serif;
	color:#333;
	letter-spacing:2px;
}
html, body, p, h1, h2, h3, h4, h5, h6, ul, li, dl, dt, dd, form, button, input, textarea {
	margin:0;
	padding:0;
}
table td {
	table-layout:fixed;
}
.patient_info table {
	width:345px;
	margin:0 auto;
}
.patient_info table tr td {
	height:38px;
	font-size:20px;
	font-weight:600;
}
.page {
	margin:0px auto 0 auto;
	width:750px;
}

.logo {
	width:445px;
	height:78px;
	text-align:left;
}
.hospital_name {
	width:550px;
	height:110px;
	margin:80px auto 90px auto;
}
.under_line {
	float:center;
	text-align:center;
	border-bottom:1px solid #333;
	position:relative;
	cursor:pointer;
	width:226px;
	font-size:16px;
	height:20px;
}
.hospital_img {
	width:677px;
	height:500px;
	margin:85px auto 0 auto;
}
.tijian_jieshao {
	width:750px;
	height:1130px;
}
.page_header {
	width:750px;
	height:41px;
	margin-bottom:10px;
	margin-top:10px;
}
.zongjian_title {
	width:750px;
	height:176px;
}
.summary_table {
	width:750px;
	border:1px solid;
	font-size:14px;
	color:#000;
}
.summary_title {
	font-size:18px;
	height:42px;
	line-height:42px;
	padding:0 5px;
	background:#4bacc6;
}
.summary_column_title td {
	height:22px;
	line-height:22px;
	padding-left:5px;
}
.summary_column_result td {
	padding:3px 0 0 5px;
	padding-bottom:20px;
}
.column_title {
	border-bottom:1px solid #4bacc6;
}
.text_content{
	line-height:20px;
	}
.summary_column_intro td {
	height:32px;
	line-height:42px;
	font-size:16px;
	text-align:center;
	border-top:1px solid #4bacc6;
	border-bottom:1px solid #4bacc6;
}
.yishi_qianming {
	font-size:16px;
	font-weight:bold;
	padding-left:200px;
	height:35px;
	line-height:35px;
	background:#92cddc;
	text-align:right
}
.table_title{
	font-size:20px;
	font-weight:bold;
	line-height:1.5;
	}
.baogao_table {
	width:750px;
	border-top:1px solid #000;
	border-left:1px solid #000;
	font-size:14px;
}
.baogao_table tr td {
	height:25px;
	border-bottom:1px solid #000;
	border-right:1px solid #000;
	white-space:nowrap;
	padding-left:5px
}
.baogao_table_title {
	background:#b6dde8;
}

.baogao_table_sub_title {
	height:28px;
	font-size:15px;
	font-weight:bold;
	border:none;
}

.baogao_table_footer {
	width:100%;
	font-size:14px;
	font-weight:600;
	margin-bottom:25px;
	height:22px;
	line-height:22px;
}

.jieguo_shuoming{
	color:#F00;
	}

.zhongxin_jianjie {
	width:750px;
	height:493px;
}
.yixue_shengming {
	width:750px;
	height:355px;
}
.futu {
	font-size:20px;
	font-weight:bolder;
	text-align:left;
	border-bottom:2px solid #000;
	padding:0 0 5px 22px;
	margin-top:10px;
}

.quicklink  { position:fixed;width:80px;display:block; top:0;right:0; border-bottom:1px solid #d9f3d8;}
.quicklink ul { list-style:none;}
.quicklink li a{ display:block; line-height:30px;background:#f1fef0; width:80px; color:#2e74d3; 
                   border-left:1px solid #d9f3d8;border-right:1px solid #d9f3d8;border-top:1px solid #d9f3d8; text-align:center; }
.quicklink li a:hover { background:#ddf6dc;}

.textarea{
	width:730px;
	height:auto;
	padding:10px;
	margin:10px;
	font-size:16px;
	overflow:hidden;
	line-height:1.5;
}

.page  .head_title {
	text-align:center;
	margin-bottom:20px;
	font-weight:bold;
}
.page  .head_title .main_title {
	letter-spacing:25px;
	font-size:28px;
	height:35px;
}
.page  .head_title .sub_title {
	letter-spacing:10px;
	font-size:32px;
	height:45px;
}

.page .head_table {
	table-layout:fixed;
	font-size:16px;
	line-height:25px;
	width:750px;
	margin-bottom:15px
}

.page .head_table .info_area {
	border-bottom:1px solid #000;
	text-align:left;
	padding-left:2px;
}
.page .head_table .info_title {
	text-align:right;
	padding-right:2px;
}

.page .footer_table {
	table-layout:fixed;
	font-size:16px;
	line-height:25px;
	width:750px;
	margin-top:10px
}

.page .footer_table .info_area {
	border-bottom:1px solid #000;
	text-align:left;
	padding-left:2px;
}
.page .footer_table .info_title {
	text-align:right;
	padding-right:2px;
}

.page .baogao_table {
	width:730px;
	border-top:2px solid #000;
	border-bottom:2px solid #000;
	border-left:none
	margin-left:10px
	font-size:14px;
}
.td_title {
	font-weight:bold;
	text-align:center;
	height:25px;	
}
.td_content {
  text-align:left;
	height:25px;	
}

.page .baogao_table_title {
	background:#b6dde8;
}

.jieguo_shuoming{
	color:#F00;
}

.shuoming{
	font-weight:bold;
	padding-top:20px;
	font-size:16px;
}

.baogao_foot{
	width:730px;
	font-size:14px;
	margin:0;
}

.xiangmu_title{
	font-size:20px;
}