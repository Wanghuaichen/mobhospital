.one_page_jianyan_jiancha_baogao {
	margin:0px auto 0 auto;
	padding-top:40px;
	width:760px;
	overflow:hidden;
}
.one_page_jianyan_jiancha_baogao  .head_title {
	text-align:center;
	margin-bottom:20px;
	font-weight:bold;
}
.one_page_jianyan_jiancha_baogao  .head_title .main_title {
	letter-spacing:10px;
	text-align:center;
	font-size:32px;
	min-height:45px;
}
.one_page_jianyan_jiancha_baogao  .head_title .sub_title_report {
	letter-spacing:5px;
	font-size:25px;
}
.one_page_jianyan_jiancha_baogao .head_table {
	table-layout:fixed;
	font-size:16px;
	line-height:25px;
	width:750px;
	margin-bottom:15px
}
.one_page_jianyan_jiancha_baogao .head_table .info_area {
	border-bottom:1px solid #000;
	text-align:left;
	padding-left:2px;
}
.one_page_jianyan_jiancha_baogao .head_table .info_title {
	text-align:right;
	padding-right:2px;
}
.one_page_jianyan_jiancha_baogao .footer_table {
	table-layout:fixed;
	font-size:16px;
	line-height:25px;
	width:750px;
	margin-top:10px
}
.one_page_jianyan_jiancha_baogao .footer_table .info_area {
	border-bottom:1px solid #000;
	text-align:left;
	padding-left:2px;
}
.one_page_jianyan_jiancha_baogao .footer_table .info_title {
	text-align:right;
	padding-right:2px;
}
.one_page_jianyan_jiancha_baogao .baogao_table {
	margin-bottom:15px;
	width:740px;
	table-layout:fixed;
	font-size:16px;
	border-collapse:collapse;
	border:1px solid #000;
}
.one_page_jianyan_jiancha_baogao .baogao_table tr td {
	white-space:normal;
	height:30px;
	padding-left:5px;
	border:1px solid #000;
}
.one_page_jianyan_jiancha_baogao .baogao_table_title {
	background:#b6dde8;
}
.one_page_jianyan_jiancha_baogao .jieguo_shuoming{
	color:#F00;
	}
.one_page_jianyan_jiancha_baogao .shuoming{
	padding-top:20px;
	font-size:16px;
}