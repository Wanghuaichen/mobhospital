html, body, p, h1, h2, h3, h4, h5, h6, ul, li, dl, dt, dd, form, button, input, textarea {
	margin:0;
	padding:0;
}
body {
	margin:10px;
	background-color:#FFF;
}
.shujuyuan {
	/*text-decoration: underline;*/
	color: blue;
	cursor: pointer;
	text-decoration: none;
	border-bottom: 2px dotted ;
}
.selected {
	text-align: center;
	cursor: pointer;
	background-color: #90CA2F;
	color: #FFF;
}
.selected>td {
	min-width: 100px;
}
.shujuyuan_hide {
	display: none;
	/*visibility: hidden;*/
}
.shujuyuan_tr {
	text-align: center;
	background-color: #EFE;
}
.shujuyuan_tr>td{
	min-width: 100px;
	cursor: pointer;
	border: thin solid #90CA2F;
}
[alt="editing"] {
	color: white;
	background-color: orange;
